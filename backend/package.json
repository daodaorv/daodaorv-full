{
  "name": "daodao-rv-backend",
  "version": "1.0.0",
  "description": "DaoDaoRV Backend API Service",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --transpile-only src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "sync-db": "ts-node scripts/sync-db.ts",
    "seed:mock": "ts-node src/scripts/seed-mock-data.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d ormconfig.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ormconfig.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ormconfig.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d ormconfig.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "rv",
    "rental",
    "koa",
    "typescript"
  ],
  "author": "DaoDaoRV Team",
  "license": "MIT",
  "dependencies": {
    "@koa/multer": "^4.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/koa__multer": "^2.0.8",
    "@types/koa-static": "^4.0.4",
    "@types/node-cron": "^3.0.11",
    "@types/sharp": "^0.31.1",
    "ali-oss": "^6.23.0",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.0",
    "dayjs": "^1.11.0",
    "exceljs": "^4.4.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.14.0",
    "koa-body": "^6.0.1",
    "koa-cors": "^0.0.16",
    "koa-jwt": "^4.0.4",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.6.0",
    "node-cron": "^4.2.1",
    "redis": "^4.6.0",
    "sharp": "^0.34.4",
    "socket.io": "^4.6.0",
    "typeorm": "^0.3.17",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.16.11",
    "@types/bcryptjs": "^2.4.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/koa": "^2.13.0",
    "@types/koa-cors": "^0.0.5",
    "@types/koa-router": "^7.4.0",
    "@types/lodash": "^4.14.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
